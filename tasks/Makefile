# -*- Makefile -*-

####################################################################################################

all: image-browser.rcc ImageBrowserRessource.py

####################################################################################################

%.rcc : %.qrc
	rcc-qt5 -binary $< -o $@

ImageBrowserRessource.py : image-browser.qrc
	pyrcc5 -o $@ $<

####################################################################################################

clean:
	rm *.py *.rcc
